[core]
	excludesfile = /Users/dundeereyno/.gitignore_global
[difftool "sourcetree"]
	cmd = opendiff \"$LOCAL\" \"$REMOTE\"
	path = 
[mergetool "sourcetree"]
	cmd = /Applications/Sourcetree.app/Contents/Resources/opendiff-w.sh \"$LOCAL\" \"$REMOTE\" -ancestor \"$BASE\" -merge \"$MERGED\"
	trustExitCode = true
[user]
	name = Dundee L P Reyno
	email = dlpreyno@gmail.com
[commit]
	template = /Users/dundeereyno/.stCommitMsg
[alias]
	logline = log --pretty=format:'%C(yellow)%h%Creset %C(cyan)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset'
